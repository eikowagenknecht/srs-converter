{
  "name": "srs-converter",
  "version": "0.3.0",
  "type": "module",
  "packageManager": "pnpm@10.19.0",
  "engines": {
    "pnpm": ">=10.0.0",
    "node": ">=22.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "lefthook",
      "protobufjs",
      "unrs-resolver",
      "esbuild"
    ]
  },
  "description": "A library for converting between different spaced repetition system formats",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "license": "MIT",
  "author": "Eiko Wagenknecht <git@eiko-wagenknecht.de> (https://eikowagenknecht.de)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eikowagenknecht/srs-converter.git"
  },
  "bugs": {
    "url": "https://github.com/eikowagenknecht/srs-converter/issues"
  },
  "homepage": "https://github.com/eikowagenknecht/srs-converter#readme",
  "keywords": [
    "srs",
    "spaced-repetition",
    "flashcards",
    "converter",
    "anki",
    "apkg",
    "typescript"
  ],
  "files": [
    "dist/",
    "!dist/**/*.test.*",
    "!dist/docs/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsdown src/index.ts",
    "format": "prettier --write ./src ./docs && biome format --write ./src ./docs",
    "lint": "pnpm type-check && pnpm lint:eslint && pnpm lint:prettier && pnpm lint:biome",
    "lint:biome": "biome check --write ./src ./docs",
    "lint:eslint": "eslint --fix ./src ./docs",
    "lint:prettier": "prettier --write ./src ./docs",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:dist": "pnpm build && pnpm test:dist:node && pnpm test:dist:bun && pnpm test:dist:deno",
    "test:dist:node": "node tests/dist/read-compatibility.mjs",
    "test:dist:bun": "bun tests/dist/read-compatibility.mjs",
    "test:dist:deno": "deno run --allow-all tests/dist/read-compatibility.mjs",
    "clean:npm": "pnpm dlx rimraf -rf node_modules",
    "sync:npm": "pnpm i --frozen-lockfile",
    "upgrade:npm": "pnpm dlx rimraf pnpm-lock.yaml && pnpm up --latest && pnpm i",
    "release:dry": "GITHUB_TOKEN=$(gh auth token) semantic-release --dry-run --no-ci",
    "knip": "knip",
    "jscpd": "jscpd ./src"
  },
  "dependencies": {
    "archiver": "7.0.1",
    "kysely": "0.28.9",
    "kysely-wasm": "1.2.1",
    "protobufjs": "8.0.0",
    "sql.js": "1.13.0",
    "unzipper": "0.12.3",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@commitlint/cli": "20.3.0",
    "@commitlint/config-conventional": "20.3.0",
    "@commitlint/types": "20.2.0",
    "@eslint/js": "9.39.2",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/archiver": "7.0.0",
    "@types/node": "24.10.1",
    "@types/sql.js": "1.4.9",
    "@types/unzipper": "0.10.11",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "9.39.2",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-jsdoc": "61.5.0",
    "globals": "16.5.0",
    "jscpd": "4.0.5",
    "knip": "5.80.0",
    "lefthook": "2.0.13",
    "prettier": "3.7.4",
    "semantic-release": "25.0.2",
    "tsdown": "0.18.4",
    "typescript": "5.9.3",
    "typescript-eslint": "8.52.0",
    "vitest": "3.2.4"
  }
}
