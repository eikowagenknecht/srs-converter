{
  "name": "srs-converter",
  "version": "0.3.0",
  "description": "A library for converting between different spaced repetition system formats",
  "keywords": [
    "anki",
    "apkg",
    "converter",
    "flashcards",
    "spaced-repetition",
    "srs",
    "typescript"
  ],
  "homepage": "https://github.com/eikowagenknecht/srs-converter#readme",
  "bugs": {
    "url": "https://github.com/eikowagenknecht/srs-converter/issues"
  },
  "license": "MIT",
  "author": "Eiko Wagenknecht <git@eiko-wagenknecht.de> (https://eikowagenknecht.de)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eikowagenknecht/srs-converter.git"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist/",
    "!dist/**/*.test.*",
    "!dist/docs/"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsdown src/index.ts",
    "format": "oxfmt",
    "lint": "pnpm type-check && pnpm lint:oxlint && pnpm lint:oxfmt",
    "lint:oxlint": "oxlint --fix",
    "lint:oxfmt": "oxfmt ./src ./docs",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:dist": "pnpm build && pnpm test:dist:node && pnpm test:dist:bun && pnpm test:dist:deno",
    "test:dist:node": "node tests/dist/read-compatibility.mjs",
    "test:dist:bun": "bun tests/dist/read-compatibility.mjs",
    "test:dist:deno": "deno run --allow-all tests/dist/read-compatibility.mjs",
    "clean:npm": "pnpm dlx rimraf -rf node_modules",
    "sync:npm": "pnpm i --frozen-lockfile",
    "upgrade:npm": "pnpm dlx rimraf pnpm-lock.yaml && pnpm up --latest && pnpm i",
    "release:dry": "GITHUB_TOKEN=$(gh auth token) semantic-release --dry-run --no-ci",
    "knip": "knip",
    "jscpd": "jscpd ./src"
  },
  "dependencies": {
    "archiver": "7.0.1",
    "kysely": "0.28.11",
    "kysely-wasm": "1.2.1",
    "protobufjs": "8.0.0",
    "sql.js": "1.14.0",
    "unzipper": "0.12.3",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "20.4.1",
    "@commitlint/config-conventional": "20.4.1",
    "@commitlint/types": "20.4.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/archiver": "7.0.0",
    "@types/node": "24.10.1",
    "@types/sql.js": "1.4.9",
    "@types/unzipper": "0.10.11",
    "@vitest/coverage-v8": "3.2.4",
    "globals": "17.3.0",
    "jscpd": "4.0.8",
    "knip": "5.83.1",
    "lefthook": "2.1.1",
    "oxfmt": "0.33.0",
    "oxlint": "^1.41.0",
    "semantic-release": "25.0.3",
    "tsdown": "0.20.3",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "packageManager": "pnpm@10.28.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "lefthook",
      "protobufjs",
      "unrs-resolver",
      "esbuild"
    ]
  }
}
